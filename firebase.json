{
  "hosting": {
    "public": "www",
    "ignore": [
      "firebase.json",
      "**/.*",            
      "**/node_modules/**", 
      "**/*.exe",         
      "**/*.bat",      
      "**/*.sh",         
      "**/*.cmd"           
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}

